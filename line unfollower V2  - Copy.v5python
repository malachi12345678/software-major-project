{"mode":"Text","hardwareTarget":"brain","textContent":"#region VEXcode Generated Robot Configuration\nfrom vex import *\nimport urandom\nimport math\n\n# Brain should be defined by default\nbrain=Brain()\n\n# Robot configuration code\nLTL = Line(brain.three_wire_port.a)\nLTM = Line(brain.three_wire_port.b)\nLTR = Line(brain.three_wire_port.c)\nML = Motor(Ports.PORT1, GearSetting.RATIO_18_1, True)\nMR = Motor(Ports.PORT2, GearSetting.RATIO_18_1, False)\nLTLL = Line(brain.three_wire_port.g)\nLTRR = Line(brain.three_wire_port.h)\n\n\n# wait for rotation sensor to fully initialize\nwait(30, MSEC)\n\n\n# Make random actually random\ndef initializeRandomSeed():\n    wait(100, MSEC)\n    random = brain.battery.voltage(MV) + brain.battery.current(CurrentUnits.AMP) * 100 + brain.timer.system_high_res()\n    urandom.seed(int(random))\n      \n# Set random seed \ninitializeRandomSeed()\n\n\ndef play_vexcode_sound(sound_name):\n    # Helper to make playing sounds from the V5 in VEXcode easier and\n    # keeps the code cleaner by making it clear what is happening.\n    print(\"VEXPlaySound:\" + sound_name)\n    wait(5, MSEC)\n\n# add a small delay to make sure we don't print in the middle of the REPL header\nwait(200, MSEC)\n# clear the console to make sure we don't have the REPL in the console\nprint(\"\\033[2J\")\n\n#endregion VEXcode Generated Robot Configuration\n\n#maze logic right hand rule follow rhs of maze\n#win condition dark black line < 20 percent reflection\nk_speed =0.9\ncount = 0\ndef DF():\n    MR.set_velocity(k_speed*5, PERCENT)\n    ML.set_velocity(k_speed*5, PERCENT)\n    MR.spin(FORWARD)\n    ML.spin(FORWARD)\n\ndef DR():\n    MR.set_velocity(k_speed*4, PERCENT)\n    ML.set_velocity(k_speed*4, PERCENT)\n    MR.spin(REVERSE)\n    ML.spin(REVERSE)\n\ndef TSR():\n    MR.set_velocity(k_speed*2.5, PERCENT)\n    ML.set_velocity(k_speed*5, PERCENT)\n    MR.spin(REVERSE)\n    ML.spin(FORWARD)\n\ndef TSL():\n    MR.set_velocity(k_speed*5, PERCENT)\n    ML.set_velocity(k_speed*2.5, PERCENT)\n    MR.spin(FORWARD)\n    ML.spin(REVERSE)\n\ndef TL():\n    MR.set_velocity(k_speed*5, PERCENT)\n    ML.set_velocity(k_speed*5, PERCENT)\n    MR.spin(REVERSE)\n    ML.spin(FORWARD)\n\ndef TR():\n    MR.set_velocity(k_speed*5, PERCENT)\n    ML.set_velocity(k_speed*5, PERCENT)\n    MR.spin(FORWARD)\n    ML.spin(REVERSE)\n\ndef get_ratio(turncount, drivecount):\n    turnratio = ((2 * drivecount) / (turncount))\n    return turnratio\n\ndef turn_direction(lefts, rights):\n    #if + left turn, if - right\n    turnsum = ((lefts) - (rights)) \n    #right = true\n    if turnsum <= 0:\n        return True\n    else:\n        return False\n\n\n\ndef is_stuck(turnratio):\n    k = 7\n    if turnratio <= k:\n        return True\n    else:\n        return False\n\n\n#right hand rule, sensing lhs of line\ndef main():\n    lefts = 0\n    rights = 0\n    count = 0\n    while True:\n        if count >= 20:\n            print(turn_direction(lefts, rights))\n            count = 0\n            rights = 0\n            count = 0\n        else:\n            count += 1\n    \n        readout()\n        if LTRR.reflectivity(PERCENT) > 80 and LTR.reflectivity(PERCENT) < 80:\n            DF()\n            wait(0.1,SECONDS)\n          \n        elif LTRR.reflectivity(PERCENT) < 30 and LTR.reflectivity(PERCENT) < 30: #win condition\n            DF()\n            wait(1,SECONDS)\n            DR()\n            wait(0.5,SECONDS)\n            TL()\n            wait(0.5,SECONDS)\n            TR()\n            wait(0.5,SECONDS)\n            TL()\n            wait(0.5,SECONDS)\n            TR()\n            wait(0.5,SECONDS)\n            TL()\n            wait(0.5,SECONDS)\n            TR()\n            wait(0.5,SECONDS)\n            MR.stop()\n            ML.stop()\n            sys.exit()\n        elif LTRR.reflectivity(PERCENT) > 80 and LTR.reflectivity(PERCENT) > 80:\n            lefts += 1\n            print(\"lefts\" , lefts)\n            DR()\n            wait(0.1,SECONDS)\n            TL()\n            wait(0.1,SECONDS)\n            \n        elif LTRR.reflectivity(PERCENT) < 80 and LTR.reflectivity(PERCENT) < 80:\n            rights += 1\n            print(\"rights\" , rights)\n            DF()\n            wait(0.1,SECONDS)\n            TR()\n            wait(0.1,SECONDS)\n\n\n\n            \n    \n\ndef readout():\n    #ML.set_stopping(COAST)\n    #MR.set_stopping(COAST)\n    #while True:\n    ltll = LTLL.reflectivity(PERCENT)\n    ltrr = LTRR.reflectivity(PERCENT)\n    ltr = LTR.reflectivity(PERCENT)\n    #brain.screen.set_cursor(1,1)\n    #brain.screen.print(ltll)\n    brain.screen.set_cursor(2,1)\n    brain.screen.print(ltr)\n    brain.screen.set_cursor(3,1)\n    brain.screen.print(ltrr)\n    #print(turn_direction(lefts, rights))\n    wait(0.1,SECONDS)\n    brain.screen.clear_screen()\n\n\n\n\nmain()\n","textLanguage":"python","robotConfig":[{"port":[1],"name":"LTL","customName":true,"deviceType":"LineTracker","deviceClass":"line","setting":{},"triportSourcePort":22},{"port":[2],"name":"LTM","customName":true,"deviceType":"LineTracker","deviceClass":"line","setting":{},"triportSourcePort":22},{"port":[3],"name":"LTR","customName":true,"deviceType":"LineTracker","deviceClass":"line","setting":{},"triportSourcePort":22},{"port":[1],"name":"ML","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[2],"name":"MR","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[7],"name":"LTLL","customName":true,"deviceType":"LineTracker","deviceClass":"line","setting":{},"triportSourcePort":22},{"port":[8],"name":"LTRR","customName":true,"deviceType":"LineTracker","deviceClass":"line","setting":{},"triportSourcePort":22}],"slot":0,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.60.0","minVersion":"3.1.0","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false,"aiVisionSettings":{"colors":[],"codes":[],"tags":true,"AIObjects":true,"AIObjectModel":[],"aiModelDropDownValue":null},"target":"Physical"}